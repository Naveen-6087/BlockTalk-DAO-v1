{
  "name": "frontend",
  "version": "0.1.0",
  "homepage": ".",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@ethersproject/solidity": "^5.7.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.7",
    "@splinetool/react-spline": "^4.1.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "antd": "^5.7.0",
    "axios": "^1.4.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "ethers": "^5.7.2",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0",
    "react-scripts": "5.0.1",
    "tailwind-merge": "^1.14.0",
    "web-vitals": "^3.3.1"
  },
  "scripts": {
    "dev:css": "tailwindcss -i ./src/index.css -o ./src/output.css --watch",
    "start": "set BROWSER=none && set PUBLIC_URL=./ && concurrently \"npm:dev:css\" \"react-scripts start\"",
    "build:css": "tailwindcss -i ./src/index.css -o ./src/output.css",
    "build": "npm run build:css && set \"CI=\" && set \"DISABLE_ESLINT_PLUGIN=true\" && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17"
  }
}
